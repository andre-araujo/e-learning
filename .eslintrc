{
  "parser": "babel-eslint",
  "plugins": [
    "react",
    "babel"
  ],
  "settings": {
    "ecmascript": 6,
    "jsx": true
  },
  "env": {
    "node": true,
    "es6": true
  },
  "globals": {
    "document": true,
    "test": true,
    "expect": true,
    "describe": true,
    "it": true,
    "fetch": true,
    "jest": true,
    "beforeEach": true,
    "window": true,
    "app": true,
    "FormData": true
  },
  "rules": {
    "import/prefer-default-export": 0,
    "import/no-named-as-default-member": 0,
    "import/no-extraneous-dependencies": 0,
    "global-require": 0,
    "consistent-return": 0,
    "no-restricted-syntax": 0,
    "indent": [
      1,
      2,
      {
        "SwitchCase": 1
      }
    ],
    "linebreak-style": [
      0
    ],
    "no-multi-assign": [
      0
    ],
    "camelcase": 0,
    "react/jsx-filename-extension": [
      0
    ],
    "react/jsx-indent": [
      1,
      2
    ],
    "react/jsx-indent-props": [
      1,
      2
    ],
    "react/react-in-jsx-scope": 0,
    "react/prop-types": 0
  },
  "extends": "airbnb"
}
