{
  "name": "e-learning",
  "version": "0.0.1",
  "description": "E-learning app created with NextJS",
  "main": "index.js",
  "engines": {
    "node": ">=8.11.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/andre-araujo/e-learning"
  },
  "author": "andre-araujo <andreluis9214@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "next build src",
    "export": "npm run build && next export src -o dist",
    "commit": "git-cz",
    "lint:fix": "eslint ./ --fix",
    "lint": "eslint ./",
    "dev": "cross-env NODE_ENV=development nodemon ./server.js",
    "start": "cross-env NODE_ENV=production node ./server.js",
    "test": "jest --clearCache && jest",
    "test:coverage": "npm test -- --coverage",
    "test:watch": "npm test -- --watch"
  },
  "dependencies": {
    "babel-plugin-styled-components": "1.5.1",
    "body-parser": "^1.18.3",
    "cross-env": "5.2.0",
    "crypto-js": "^3.1.9-1",
    "dlv": "^1.1.2",
    "dotenv": "6.0.0",
    "express": "4.16.2",
    "express-fileupload": "^1.0.0-alpha.1",
    "grid-styled": "5.0.2",
    "isomorphic-unfetch": "2.1.1",
    "jsonwebtoken": "^8.3.0",
    "moment": "^2.22.2",
    "mongoose": "^5.2.6",
    "multer": "^1.3.1",
    "next": "6.1.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "pdfkit": "^0.8.3",
    "prop-types": "15.6.0",
    "react": "16.4.1",
    "react-dom": "16.4.1",
    "react-icons": "^3.0.4",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-create-request": "^3.0.0",
    "redux-devtools-extension": "^2.13.5",
    "redux-loading-middleware": "^1.0.1",
    "redux-thunk": "^2.3.0",
    "reselect": "^3.0.1",
    "styled-components": "3.3.3",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "babel-eslint": "8.2.6",
    "babel-jest": "^23.6.0",
    "babel-core": "7.0.0-bridge.0",
    "commitizen": "2.10.1",
    "cz-conventional-changelog": "2.1.0",
    "enzyme": "3.4.3",
    "enzyme-adapter-react-16": "1.2.0",
    "enzyme-to-json": "3.3.4",
    "eslint": "5.2.0",
    "eslint-config-airbnb": "17.0.0",
    "eslint-plugin-babel": "5.1.0",
    "eslint-plugin-import": "2.13.0",
    "eslint-plugin-jsx-a11y": "6.1.1",
    "eslint-plugin-react": "7.10.0",
    "jest": "23.5.0",
    "nodemon": "^1.18.3",
    "raf": "3.4.0",
    "react-addons-test-utils": "15.6.2",
    "react-test-renderer": "16.4.2"
  },
  "jest": {
    "verbose": true,
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "transformIgnorePatterns": [
      "/node_modules/"
    ],
    "collectCoverageFrom": [
      "src/components/elements/**/*.js",
      "src/components/modules/**/*.js",
      "!src/components/**/*styles.js"
    ],
    "setupFiles": [
      "<rootDir>/jest.setup.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 70,
        "functions": 70,
        "lines": 70,
        "statements": 70
      }
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
