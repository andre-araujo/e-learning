{
  "name": "e-learning",
  "version": "0.0.1",
  "description": "E-learning app created with NextJS",
  "main": "index.js",
  "engines": {
    "node": ">=9.10.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/andre-araujo/e-learning"
  },
  "author": "andre-araujo <andreluis9214@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "next build",
    "commit": "git-cz",
    "dev": "node client.js",
    "lint:fix": "eslint ./ --fix",
    "lint": "eslint ./",
    "api": "node ./api/index.js",
    "api:watch": "nodemon ./api/index.js",
    "start": "cross-env NODE_ENV=production node client.js",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "babel-plugin-styled-components": "1.5.1",
    "body-parser": "^1.18.3",
    "crypto-js": "^3.1.9-1",
    "dlv": "^1.1.2",
    "dotenv": "~6.0.0",
    "express": "~4.16.2",
    "grid-styled": "~5.0.2",
    "isomorphic-unfetch": "~2.1.1",
    "jsonwebtoken": "^8.3.0",
    "moment": "^2.22.2",
    "mongoose": "^5.2.6",
    "next": "~6.1.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "prop-types": "~15.6.0",
    "react": "~16.4.1",
    "react-dom": "~16.4.1",
    "react-icons": "^3.0.4",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-create-request": "^3.0.0",
    "redux-loading-middleware": "^1.0.1",
    "redux-thunk": "^2.3.0",
    "reselect": "^3.0.1",
    "styled-components": "~3.3.3",
    "uuid": "^3.3.2",
    "cross-env": "~5.2.0"
  },
  "devDependencies": {
    "babel-eslint": "~8.2.6",
    "commitizen": "~2.10.1",
    "cz-conventional-changelog": "~2.1.0",
    "enzyme": "~3.3.0",
    "enzyme-adapter-react-16": "~1.1.1",
    "enzyme-to-json": "~3.3.4",
    "eslint": "~5.2.0",
    "eslint-config-airbnb": "~17.0.0",
    "eslint-plugin-babel": "~5.1.0",
    "eslint-plugin-import": "~2.13.0",
    "eslint-plugin-jsx-a11y": "~6.1.1",
    "eslint-plugin-react": "~7.10.0",
    "jest": "~23.4.2",
    "nodemon": "^1.18.3",
    "raf": "~3.4.0",
    "react-addons-test-utils": "~15.6.0",
    "react-test-renderer": "~16.4.1",
    "redux-devtools-extension": "^2.13.5"
  },
  "jest": {
    "verbose": true,
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "transformIgnorePatterns": [
      "/node_modules/"
    ],
    "collectCoverageFrom": [
      "components/elements/**/*.js",
      "components/modules/**/*.js",
      "!components/**/styles.js"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/shim.js",
    "coverageThreshold": {
      "global": {
        "branches": 70,
        "functions": 70,
        "lines": 70,
        "statements": 70
      }
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
